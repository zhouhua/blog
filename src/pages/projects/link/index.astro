---
import Container from '@components/Container.astro';
import ProjectTitle from '@components/ProjectTitle.astro';
import { HOME } from '@consts';
import projectList from '@content/projects/list';
import PageLayout from '@layouts/PageLayout.astro';
import ShortLink from './_link';

const project = projectList[9]!;
---

<PageLayout description={project.description} title={`${HOME.TITLE} - ${project.name}`}>
  <ProjectTitle description={project.description} title={project.name} />
  <Container>
    <ShortLink client:only="react" />
  </Container>
</PageLayout>
